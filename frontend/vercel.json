{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/autowash-hub/browser",
  "framework": "angular",
  "analytics": {
    "enabled": true
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://brown-octopus-872555.hostingersite.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
